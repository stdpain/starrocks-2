set pipeline_dop = 1;
select count(c_name),count(c_address) from customer;
-- shared morsel queue
set pipeline_dop = 2;
select count(c_name),count(c_address) from customer;
set pipeline_dop = 4;
select count(c_name),count(c_address) from customer;
set pipeline_dop=90;
select count(lo_orderkey),count(lo_linenumber) from lineorder;